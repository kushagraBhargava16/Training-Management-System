	</navigation-rule>
	<navigation-rule>
		<display-name>trainerDetailedCourse.xhtml</display-name>
		<from-view-id>/trainerDetailedCourse.xhtml</from-view-id>
		<navigation-case>
			<to-view-id>/trainerDetailedCourse.xhtml</to-view-id>
			<redirect />
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<display-name>index.xhtml</display-name>
		<from-view-id>/index.xhtml</from-view-id>
		<navigation-case>
			<from-outcome>register.xhtml</from-outcome>
			<to-view-id>/register.xhtml</to-view-id>
			<redirect />
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<display-name>index.xhtml</display-name>
		<from-view-id>/index.xhtml</from-view-id>
		<navigation-case>
			<from-outcome>traineeHome</from-outcome>
			<to-view-id>/traineeHomeComponent.xhtml</to-view-id>
			<redirect />
		</navigation-case>
	</navigation-rule>

</faces-config>
