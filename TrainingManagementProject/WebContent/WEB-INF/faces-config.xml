			<redirect />
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<display-name>trainerHome.xhtml</display-name>
		<from-view-id>/trainerHome.xhtml</from-view-id>
		<navigation-case>
			<from-outcome>trainerDetailedCourse</from-outcome>
