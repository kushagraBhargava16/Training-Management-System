			<from-outcome>detailedCourse</from-outcome>
			<to-view-id>/detailedCourse.xhtml</to-view-id>
			<redirect />
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<display-name>index.xhtml</display-name>
		<from-view-id>/index.xhtml</from-view-id>
		<navigation-case>
			<from-outcome>trainerHome</from-outcome>
